<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.finalproject_ningavimarie.TableActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFF">

        <ImageView
            android:id="@+id/table1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/rec1"/>

        <ImageView
            android:id="@+id/table2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table1"
            android:layout_alignStart="@+id/table1"
            android:layout_below="@+id/table1"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table3"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table2"
            android:layout_alignStart="@+id/table2"
            android:layout_centerVertical="true"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table3"
            android:layout_alignStart="@+id/table3"
            android:layout_below="@+id/table3"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table5"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table4"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/table4"
            android:layout_marginBottom="22dp"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table6"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/table1"
            android:layout_centerHorizontal="true"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table7"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table6"
            android:layout_alignStart="@+id/table6"
            android:layout_below="@+id/table6"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table8"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table7"
            android:layout_alignStart="@+id/table7"
            android:layout_below="@+id/table7"
            android:layout_marginTop="14dp"
            android:src="@drawable/rec1" />

        <ImageView
            android:id="@+id/table9"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignLeft="@+id/table8"
            android:layout_alignStart="@+id/table8"
            android:layout_alignTop="@+id/table5"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:src="@drawable/heart1" />

        <ImageView
            android:id="@+id/table10"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/table6"
            android:layout_marginLeft="19dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="21dp"
            android:layout_toEndOf="@+id/table6"
            android:layout_toRightOf="@+id/table6"
            android:src="@drawable/circle" />

        <ImageView
            android:id="@+id/table11"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/table10"
            android:layout_alignStart="@+id/table10"
            android:layout_below="@+id/table10"
            android:layout_marginTop="37dp"
            android:src="@drawable/circle" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/table11"
            android:layout_alignRight="@+id/table11"
            android:layout_below="@+id/table9"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="13dp"
            android:text="Entraince" />
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
