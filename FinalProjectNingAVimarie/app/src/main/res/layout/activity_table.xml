<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.finalproject_ningavimarie.TableActivity"
    android:background="#FFFF">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="15dp">

            <ImageView
                android:id="@+id/table1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_above="@+id/table8"
                android:layout_alignLeft="@+id/table1"
                android:layout_alignStart="@+id/table1"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@+id/table2"
                android:layout_alignStart="@+id/table2"
                android:layout_below="@+id/table8"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table4"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@+id/table4"
                android:layout_alignStart="@+id/table4"
                android:layout_below="@+id/table4"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignTop="@+id/table1"
                android:layout_centerHorizontal="true"
                android:src="@drawable/rec1" />



        </LinearLayout>


        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/table6"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table7"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_above="@+id/table8"
                android:layout_alignLeft="@+id/table1"
                android:layout_alignStart="@+id/table1"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table8"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@+id/table2"
                android:layout_alignStart="@+id/table2"
                android:layout_below="@+id/table8"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table9"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@+id/table4"
                android:layout_alignStart="@+id/table4"
                android:layout_below="@+id/table4"
                android:src="@drawable/rec1" />

            <ImageView
                android:id="@+id/table10"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignTop="@+id/table1"
                android:layout_centerHorizontal="true"
                android:src="@drawable/rec1" />




        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/table22"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/heart1" />

            <ImageView
                android:id="@+id/table23"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_above="@+id/table8"
                android:layout_alignLeft="@+id/table1"
                android:layout_alignStart="@+id/table1"
                android:src="@drawable/heart1" />

            <ImageView
                android:id="@+id/table24"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@+id/table2"
                android:layout_alignStart="@+id/table2"
                android:layout_below="@+id/table8"
                android:src="@drawable/heart1" />




            <ImageView
                android:id="@+id/table25"
                android:layout_width="100dp"
                android:layout_height="100dp"

                android:src="@drawable/circle" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/table11"
                android:layout_alignRight="@+id/table11"
                android:layout_below="@+id/table9"
                android:layout_marginEnd="13dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="50dp"
                android:text="Entraince" />
        </LinearLayout>


    </LinearLayout>





</android.support.constraint.ConstraintLayout>
